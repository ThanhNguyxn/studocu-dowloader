{
    "manifest_version": 3,
    "name": "StudoCu Downloader",
    "description": "ðŸ“š Download documents from StudoCu as PDF - Bypass blur & watermark, export with selectable text!",
    "version": "1.0.1",
    "author": "ThanhNguyxn",
    "permissions": [
        "cookies",
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "*://*.studocu.com/*",
        "*://*.studocu.vn/*",
        "*://*.studocu.de/*",
        "*://*.studocu.es/*",
        "*://*.studocu.fr/*",
        "*://*.studocu.it/*",
        "*://*.studocu.nl/*",
        "*://*.studocu.pl/*",
        "*://*.studocu.pt/*",
        "*://*.studocu.co.uk/*",
        "*://*.studocu.id/*",
        "*://*.studocu.ph/*",
        "*://*.studocu.com.br/*",
        "*://*.studocu.com.mx/*",
        "*://*.studocu.com.ar/*",
        "*://*.studocu.cl/*",
        "*://*.studocu.co/*",
        "*://*.studocu.pe/*",
        "*://*.studocu.in/*",
        "*://*.studocu.com.au/*",
        "*://*.studocu.ca/*",
        "*://*.studocu.za/*",
        "*://*.studocu.se/*",
        "*://*.studocu.no/*",
        "*://*.studocu.dk/*",
        "*://*.studocu.fi/*",
        "*://*.studocu.at/*",
        "*://*.studocu.ch/*",
        "*://*.studocu.be/*",
        "*://*.studocu.ie/*",
        "*://*.studocu.sg/*",
        "*://*.studocu.my/*",
        "*://*.studocu.th/*",
        "*://*.studocu.ae/*",
        "*://*.studocu.za/*",
        "*://*.studocu.ng/*",
        "*://*.studocu.ke/*",
        "*://*.studocu.eg/*",
        "*://*.studocu.pk/*",
        "*://*.studocu.bd/*",
        "*://*.studocu.kr/*",
        "*://*.studocu.jp/*",
        "*://*.studocu.tw/*",
        "*://*.studocu.hk/*",
        "*://*.studocu.ru/*",
        "*://*.studocu.ua/*",
        "*://*.studocu.cz/*",
        "*://*.studocu.hu/*",
        "*://*.studocu.ro/*",
        "*://*.studocu.gr/*",
        "*://*.studocu.tr/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "StudoCu Downloader",
        "default_icon": {
            "16": "icons/icon16.svg",
            "48": "icons/icon48.svg",
            "128": "icons/icon128.svg"
        }
    },
    "icons": {
        "16": "icons/icon16.svg",
        "48": "icons/icon48.svg",
        "128": "icons/icon128.svg"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.studocu.com/*",
                "*://*.studocu.vn/*",
                "*://*.studocu.de/*",
                "*://*.studocu.es/*",
                "*://*.studocu.fr/*",
                "*://*.studocu.it/*",
                "*://*.studocu.nl/*",
                "*://*.studocu.pl/*",
                "*://*.studocu.pt/*",
                "*://*.studocu.co.uk/*",
                "*://*.studocu.id/*",
                "*://*.studocu.ph/*",
                "*://*.studocu.com.br/*",
                "*://*.studocu.com.mx/*",
                "*://*.studocu.in/*",
                "*://*.studocu.com.au/*"
            ],
            "css": [
                "content.css"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "print.css"
            ],
            "matches": [
                "*://*.studocu.*/*"
            ]
        }
    ]
}