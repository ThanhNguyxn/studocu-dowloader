{
    "manifest_version": 3,
    "name": "StudoCu Downloader",
    "description": "ðŸ“š Download StudoCu documents as PDF â€“ Bypass blur & watermark, export with selectable text!",
    "version": "1.1.0",
    "author": "ThanhNguyxn",
    "permissions": [
        "cookies",
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "*://*.studocu.com/*",
        "*://*.studocu.vn/*",
        "*://*.studocu.de/*",
        "*://*.studocu.es/*",
        "*://*.studocu.fr/*",
        "*://*.studocu.it/*",
        "*://*.studocu.nl/*",
        "*://*.studocu.pl/*",
        "*://*.studocu.pt/*",
        "*://*.studocu.co.uk/*",
        "*://*.studocu.id/*",
        "*://*.studocu.ph/*",
        "*://*.studocu.com.br/*",
        "*://*.studocu.com.mx/*",
        "*://*.studocu.in/*",
        "*://*.studocu.com.au/*",
        "*://*.studocu.ca/*",
        "*://*.studocu.nz/*",
        "*://*.studocu.ie/*",
        "*://*.studocu.sg/*",
        "*://*.studocu.my/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "StudoCu Downloader",
        "default_icon": {
            "16": "icons/icon16.svg",
            "48": "icons/icon48.svg",
            "128": "icons/icon128.svg"
        }
    },
    "icons": {
        "16": "icons/icon16.svg",
        "48": "icons/icon48.svg",
        "128": "icons/icon128.svg"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.studocu.com/*",
                "*://*.studocu.vn/*",
                "*://*.studocu.de/*",
                "*://*.studocu.es/*",
                "*://*.studocu.fr/*",
                "*://*.studocu.it/*",
                "*://*.studocu.nl/*",
                "*://*.studocu.pl/*",
                "*://*.studocu.pt/*",
                "*://*.studocu.co.uk/*",
                "*://*.studocu.id/*",
                "*://*.studocu.ph/*",
                "*://*.studocu.com.br/*",
                "*://*.studocu.com.mx/*",
                "*://*.studocu.in/*",
                "*://*.studocu.com.au/*",
                "*://*.studocu.ca/*",
                "*://*.studocu.nz/*",
                "*://*.studocu.ie/*",
                "*://*.studocu.sg/*",
                "*://*.studocu.my/*"
            ],
            "css": [
                "content.css"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "print.css"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}